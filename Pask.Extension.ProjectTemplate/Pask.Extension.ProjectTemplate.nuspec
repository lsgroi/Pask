<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
  <metadata>
    <id>$id$</id>
    <version>$version$</version>
    <title>$id$</title>
    <authors>$registeredorganization$</authors>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <summary>$projectname$</summary>
    <description>$projectname$</description>
    <developmentDependency>true</developmentDependency>
    <tags>Invoke-Build PowerShell BuildAutomation Pask</tags>
    <dependencies>
      <dependency id="Pask" />
    </dependencies>
  </metadata>
  <files>
    <file src="init\**" target="init" />
    <file src="scripts\*.ps1" target="scripts" exclude="scripts\*.Tests.ps1" />
    <file src="tasks\*.ps1" target="tasks" exclude="tasks\*.Tests.ps1" />
    <file src="tools\*.ps1" target="tools" />
    <file src="readme.txt" />
  </files>
</package>